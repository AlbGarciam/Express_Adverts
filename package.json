{
  "name": "nodepop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run prepareDB && npm run db && npm run fillDatabase && npm run stopDB",
    "stopDB": "mongo --port 2703 --eval \"db.getSiblingDB('admin').shutdownServer();quit()\"",
    "prepareDB": "rm -rf ./database/db && mkdir ./database/db && mkdir -p ./var/log && touch ./var/log/mongod.log",
    "db": "mongod --fork --config ./etc/mongo.conf",
    "fillDatabase": "node fill_database.js",
    "cluster": "npm run db && node ./bin/cluster.js",
    "postcluster": "npm run stopDB",
    "doc": "./node_modules/.bin/jsdoc -c jsdoc.json -R Readme.md"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cuid": "^2.1.4",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-validator": "^5.3.0",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.3.14",
    "morgan": "~1.9.0",
    "rotating-file-stream": "^1.3.9",
    "sanitize-html": "^1.19.1"
  },
  "devDependencies": {
    "docdash": "^1.0.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc": "^3.5.5",
    "jsdoc-route-plugin": "^0.1.0"
  }
}
